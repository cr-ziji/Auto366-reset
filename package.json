{
  "name": "auto366",
  "version": "0.7.0",
  "description": "天学网自动化答题工具，支持各类题型答案获取和一些自动填写",
  "main": "main.js",
  "author": "Auto366",
  "license": "GNUV3",
  "repository": {
    "type": "git",
    "url": "https://github.com/cyrilguocode/Auto366.git"
  },
  "scripts": {
    "start": "electron .",
    "build": "electron-builder build --win --publish never"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "archiver": "^7.0.1",
    "electron-updater": "^6.7.3",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.1.1",
    "node-mitmproxy-pro": "^1.1.1",
    "node-stream-zip": "^1.15.0",
    "path": "^0.12.7",
    "supabase-js": "^0.0.1-security",
    "uuid": "^11.1.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.13.3",
    "electron-updater": "^6.7.3"
  },
  "build": {
    "appId": "com.auto366.app",
    "productName": "Auto366",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!dist/**/*",
      "!.git/**/*",
      "!web/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "icon.png",
      "requestedExecutionLevel": "requireAdministrator"
    },
    "publish": {
      "provider": "github",
      "owner": "cyrilguocode",
      "repo": "Auto366"
    }
  }
}
